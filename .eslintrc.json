{
	"extends": ["plugin:security/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"security/detect-non-literal-fs-filename": "off",
		"security/detect-object-injection": "off"
	}
}
