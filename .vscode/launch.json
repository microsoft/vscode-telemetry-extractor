{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "name": "Extract Core Declarations",
            "program": "${workspaceRoot}/out/extractor.js",
            "args": [
                "--sourceDir=${workspaceRoot}/src/telemetry-sources/vscode",
                "--excludedDir=src/telemetry-sources/vscode/extensions",
                "--outputDir=${workspaceRoot}",
                "--applyEndpoints",
                "--lowerCaseEvents"

            ]
        },
        {
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "name": "Extract Extensions Declarations",
            "program": "${workspaceRoot}/out/extractor.js",
            "args": [
                "--sourceDir=${workspaceRoot}/src/telemetry-sources/",
                "--outputDir=${workspaceRoot}",
                "--applyEndpoints"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "name": "Config Test",
            "program": "${workspaceRoot}/out/extractor.js",
            "args": [
                "--config=${workspaceRoot}/src/tests/mocha/resources/test-config.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "name": "Merge Telemetry",
            "program": "${workspaceRoot}/out/merge.js"
        },
        {
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "name": "Test Declarations",
            "program": "${workspaceRoot}/out/tests/compareObjects.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Mocha All",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "args": [
                "--timeout",
                "999999",
                "--colors",
                "${workspaceRoot}/out/tests/mocha"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to process",
            "port": 9229
        }
    ]
}